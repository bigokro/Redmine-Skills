<h2><%= l(:label_qualified_assignees) %></h2> 

<%= error_messages_for 'issue' %>
	
<div id="user_list">
<% form_for :issue, @issue, :url => { :controller => "required_skills", :action => "assign_user", :issue_id => @issue } do |f| %>
	<%= render :partial => 'skills_matcher/list_users', :locals => { :f => f } %>
	<%= submit_tag l(:button_assign), :tabindex => autotab %>
<% end %>
</div>


<% html_title(l(:label_skills)) -%>
